# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'BoTree911_member' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BoTree911_member
	# IQKeyboardManager allows you to prevent issues of the keyboard sliding up and cover UITextField/UITextView without needing you to enter any code and no additional setup required
	pod 'IQKeyboardManagerSwift'
	
	# Elegant HTTP Networking in Swift
	pod 'Alamofire', '~> 4.3'

	# SwiftyJSON makes it easy to deal with JSON data in Swift
	pod 'SwiftyJSON', '3.0.0'

	# A lightwight progress HUD, with blur effect, elegant API and themes Support.
	pod 'FTProgressIndicator', '~> 1.1'

	# A UIView category that adds Android-style toast notifications to iOS.
	pod 'Toast', '~> 3.0'


  target 'BoTree911_memberTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BoTree911_memberUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
